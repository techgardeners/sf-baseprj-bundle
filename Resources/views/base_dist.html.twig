    {% block navbar %}
        
        {% render 'TechGEmenuPublicBundle:Component:navbar' %}  
        
    {% endblock %}
    
    {% block header %}
        
        <div id="header-container">
            <header>        
                {% render 'TechGEmenuPublicBundle:Component:header' %}          
            </header> 
        </div> <!-- end #header-container -->   
               
    {% endblock %}


    
    <div class="container main">
        {% block main_content %}{% endblock %}
    </div>
    
    {% block footer %}
               {% render 'TechGEmenuPublicBundle:Component:footer' %}                              
    {% endblock %}

    {% block javascripts %}
        {% if app.environment == 'prod' %}
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
            <script>window.jQuery || document.write('<script src="{{ asset('js/vendor/jquery-1.8.3.min.js') }}"><\/script>')</script>
        {% else %}
            <script src="{{ asset('js/vendor/jquery-1.8.3.min.js') }}"></script>
        {% endif %}

        

        {# % javascripts                                               
            'js/plugins.js'
            'js/vendor/bootstrap.min.js'
            
            output='js_compiled/main_compiled.js'
        %}
            <!-- script type="text/javascript" src="{{ asset_url }}"></script -->
        {% endjavascripts % #} 
            
        <script type="text/javascript" src="{{ asset('js/plugins.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/vendor/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/main.js') }}"></script>
        
        {% block ready_javascripts %}

            <!-- script type="text/javascript">
                
