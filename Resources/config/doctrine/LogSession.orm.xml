<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TechG\Bundle\SfBaseprjBundle\Entity\LogSession" table="log_session">
    <id name="id" type="string" column="id">
    </id>
    <field name="logDate" type="datetime" column="log_date" nullable="false"/>
    <field name="infoUser" type="text" column="info_user" nullable="true"/>
    <field name="infoGeo" type="text" column="info_geo" nullable="true"/>
    <field name="info" type="text" column="info" nullable="true"/>
    <field name="lastActivity" type="datetime" column="last_activity" nullable="true"/>
    <field name="sessionId" type="string" column="session_id" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
